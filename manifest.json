{
    "manifest_version": 2,
    "name": "AirBnb info downloader",
    "version": "1.0",
   
    "description": "Downloads Airbnbs info and picture from their website",
   
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["contentscript.js"]
        }
      ],
   
    "permissions": ["menus", "downloads"]
  }